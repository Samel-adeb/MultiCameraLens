<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorAccent">#008DCD</color>
    <color name="colorControlHighlight">#008DCD</color>
    <color name="colorControlNormal">#57BEEE</color>
    <color name="colorPrimary">#008DCD</color>
    <color name="colorPrimaryDark">#212121</color>
    <dimen name="fab_margin">16dp</dimen>
    <string name="app_name" translatable="false">MultiCamera Lens</string>
    <string name="id_test" translatable="false">TextTexttttt</string>
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
		<item name="colorPrimary">@color/colorPrimary</item>
		<item name="colorPrimaryDark">@color/colorPrimaryDark</item>
		<item name="colorAccent">@color/colorAccent</item>
		<item name="colorControlHighlight">@color/colorControlHighlight</item>
		<item name="colorControlNormal">@color/colorControlNormal</item>
	</style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
    <style name="AppTheme.FullScreen" parent="AppTheme">
		<item name="android:windowFullscreen">true</item>
		<item name="android:windowContentOverlay">@null</item>
	</style>
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>
    <style name="Base.Theme.Tesseract" parent="Theme.Material3.DayNight.NoActionBar">
        
        
    </style>
    <style name="Theme.Tesseract" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryVariant">@color/colorPrimary</item>
        <item name="colorOnPrimary">@color/colorPrimaryDark</item>
        
        <item name="colorSecondary">@color/colorControlHighlight </item>
        <item name="colorSecondaryVariant">@color/colorControlHighlight</item>
        <item name="colorOnSecondary">@color/colorPrimaryDark</item>
        
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        
    </style>
</resources>